{
  "name": "@develit-io/general-codes",
  "version": "1.7.0",
  "description": "General Codes",
  "repository": "@develit-io/general-codes",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "biome check",
    "lint:fix": "biome check --fix",
    "prepack": "bun run build",
    "release": "bun run build && bunx changelogen@latest --release --push && npm publish --access public",
    "test": "bun lint && bun typecheck && bun vitest run --coverage",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@types/node": "^22.12.0",
    "@vitest/coverage-v8": "^3.0.4",
    "jiti": "^2.4.2",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vitest": "^3.0.4"
  }
}
